Interface:
    LoginPage:
    SignupPage:
    HomePage:
    VitMain:
    VitFC:
    VitPoona:
    VitNesCafe:
    VitKiosk:
    VitCoffee:
    RecieptPage:
    
        
        

<LoginPage>:
    name: 'login'
    email : email
    pswd : pswd        
    canvas.before:
        Color: 
            rgba: rgba('#c1d3ffff')
        Rectangle: 
            size: self.size
            pos: self.pos
    FloatLayout:
        size: root.width, root.height  
        Button:
            text: 'New User'
            color: 0,0,0,1
            background_color: 0,0,0,0
            pos_hint: {"x":0.2, "top":0.2}
            size_hint: 0.10,0.05
            on_press: 
                root.txt_animate(self)
                app.root.current = 'signup'
        Button:
            text: 'Cashier'
            color: 0,0,0,1
            background_color: 0,0,0,0
            size_hint: 0.10,0.05
            pos_hint: {"x":0.45, "top":0.2}
        Button:
            text: 'Canteen'
            color: 0,0,0,1
            background_color: 0,0,0,0
            size_hint: 0.10,0.05
            pos_hint: {"x":0.7, "top":0.2}
        TextInput:
            id: email
            multiline: False 
            size_hint: 0.4,0.04
            pos_hint: {"x":0.3, "top":0.5}
        TextInput:
            id: pswd
            multiline: False 
            size_hint: 0.4,0.04
            pos_hint: {"x":0.3, "top":0.45}
            password: True
        Label:
            text: "Password"
            color: 0,0,0,1
            size_hint: 0.4,0.04
            pos_hint: {"x":0.05, "top":0.45}
        Label:
            text: "Email"
            color: 0,0,0,1
            size_hint: 0.4,0.04
            pos_hint: {"x":0.063, "top":0.5}
        Image:
            source: 'icontext.png'
            size_hint: 1,1
            pos_hint: {"x":0, "top":1.4}
        Label:
            text: "LogIn"
            color: 0,0,0,1
            pos_hint: {"x":0, "top":1.2}
            font_size: 50
        Button:
            text: 'Submit'
            background_color: 0,0,0,1
            size_hint: 0.10,0.05
            pos_hint: {"x":0.46, "top":0.35}
            on_press: 
                root.but_animate(self)
            on_release:
                root.verify() 
                root.manager.transition.direction = "left"               
        
