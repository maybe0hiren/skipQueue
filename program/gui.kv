        Label:
            text: "Email:"
            color: 0,0,0,1
            size_hint: 0.4,0.04
            pos_hint: {"x":0.07, "top":0.45}
        Label:
            text: "Password:"
            color: 0,0,0,1
            size_hint: 0.4,0.04
            pos_hint: {"x":0.055, "top":0.4}
        Image:
            source: 'icontext.png'
            size_hint: 1,1
            pos_hint: {"x":0, "top":1.4} 
        Button:
            text: "Create Account"
            size_hint: 0.15, 0.05
            pos_hint: {"x":0.3, "top":0.3}
            background_color: 0,0,0,1
            on_press:
                root.sign()
                root.but_animate(self)
                root.manager.transition.direction = "left"
        Button:
            text: "Login Page"
            size_hint: 0.15, 0.05
            pos_hint: {"x":0.55, "top":0.3}
            background_color: 0,0,0,1
            on_press:
                root.but_animate(self)
       
