<VitMain>:
    name: "vitmain"
    canvas.before:
        Color: 
            rgba: rgba('#c1d3ffff')
        Rectangle: 
            size: self.size
            pos: self.pos 
    FloatLayout:
        size: self.width, self.height
        Label:
            text: "VIT Main Canteen Menu"
            font_size: 50
            color: 0,0,0,1
            pos_hint: {'x':0, 'top':1.3}
        Label:
            text: "Clears your kart"
            font_size: 15
            color: 1,0,0,1
            pos_hint: {'x':0.265, 'top':0.64}
        Button:
            text:"Home Page"
            background_color: 0,0,0,1
            size_hint: 0.13,0.05
            pos_hint:{'x':0.7, 'top':0.2}
            on_press:
                root.but_animate(self)
            on_release: 
                app.root.current = 'home'
                root.manager.transition.direction = "right"
        Button:
            text:"Proceed to pay"
            background_color: 0,0,0,1
            size_hint: 0.13,0.05
            pos_hint:{'x':0.2, 'top':0.2}
            on_press:
                root.but_animate(self)
            on_release: 
                app.root.current = 'reciept'
                root.manager.transition.direction = "left"
        
        ScrollView:
            do_scroll_x: False 
            do_scroll_y: True
            size_hint: (0.5,0.5)
            pos_hint: {'x':0.265, 'top':0.73}
            BoxLayout:
